<!DOCTYPE html>
<html>
  <head>
    <title>CaseMap</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <script src="//maps.google.com/maps/api/js?v=3&sensor=false&libraries=geometry&key=<%=ENV['GOOGLE_MAPS_APIKEY']%>" type="text/javascript"></script>
    <script src='//cdn.rawgit.com/googlemaps/v3-utility-library/master/markerclustererplus/src/markerclusterer.js' type='text/javascript'></script>
    <%= analytics_init if Rails.env.production? %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
